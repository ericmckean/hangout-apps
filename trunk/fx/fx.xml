<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Hangout Poke">
		<Require feature="rpc" />
		<Require feature="views" />\
		<Require feature="locked-domain" /> \
	</ModulePrefs>
	<Content type="html"><![CDATA[     

<html>
<head>
	<title>Hangout FX App</title>
	    <meta http-equiv="Content-Type" content="text/html; charset = iso - 8859 - 1 " />

  <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.16/themes/base/jquery-ui.css" type="text/css" media="all" />
</head>

<body>

<script type="text/javascript" src="//www.google.com/jsapi"></script>
<script type="text/javascript">google.load("jquery", "1.3.2");</script>
<script type="text/javascript">google.load("jqueryui", "1.8.16");</script>
<script src="https://hangoutsapi.talkgadget.google.com/hangouts/api/hangout.js?v=0.1" ></script>


<div id="app-container" style="width:243px; border:1px solid #a0adbb">

<div id="overlay-controls" style="background:url(https://hangout-apps.googlecode.com/svn/trunk/fx/css/fx-overlays-button-bar.png);width:243px;height:44px;">
  <div id="hat-button" style="position:absolute;left:8px;top:3px;width:54px;height:42px;cursor:pointer" onclick="selectOverlayButton('hat')" onmouseover="handlePrelight('hat')" onmouseout="handlePrelight()">
    <img id="hat-prelight-img" style="display:none" src="https://hangout-apps.googlecode.com/svn/trunk/fx/css/hat-prelight.png">
    <img id="hat-button-img" style="display:none" src="https://hangout-apps.googlecode.com/svn/trunk/fx/css/hat-button.png">
  </div>
  <div id="monocle-button" style="position:absolute;left:66px;top:3px;width:54px;height:42px;cursor:pointer;" onclick="selectOverlayButton('monocle')" onmouseover="handlePrelight('monocle')" onmouseout="handlePrelight()">
    <img id="monocle-prelight-img" style="display:none" src="https://hangout-apps.googlecode.com/svn/trunk/fx/css/monocle-prelight.png">
    <img id="monocle-button-img" style="display:none" src="https://hangout-apps.googlecode.com/svn/trunk/fx/css/monocle-button.png">
  </div>
  <div id="stache-button" style="position:absolute;left:124px;top:3px;width:54px;height:42px;cursor:pointer;" onclick="selectOverlayButton('stache')" onmouseover="handlePrelight('stache')" onmouseout="handlePrelight()">
    <img id="stache-prelight-img" style="display:none" src="https://hangout-apps.googlecode.com/svn/trunk/fx/css/stache-prelight.png">
    <img id="stache-button-img" style="display:none" src="https://hangout-apps.googlecode.com/svn/trunk/fx/css/stache-button.png">
  </div>
  
</div>

<div id="sound-broadcast" style="width:243px;height:44px;border-bottom:1px solid #a0adbb">
</div>

<div id="participant-list" style="height:150px;scroll:auto">
</div>

<div id="messageDisplay" style="width:235px;border-top:1px solid #a0adbb;padding:4px;display:none">
</div>

<div id="giftDisplay" style="width:235px;border-top:1px solid #a0adbb;padding:4px;display:none">
</div>

</div>

<script src="https://google-hangouts-apps.googlecode.com/svn/trunk/fx/src/fx.js"></script>

]]>
</Content>

</Module>
